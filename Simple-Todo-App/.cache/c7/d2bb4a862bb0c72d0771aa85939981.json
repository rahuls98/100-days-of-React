\n\nbody {\n    background: rgb(200, 253, 255);\n}\n\n#root {\n    height: 97vh;\n    display: flex;\n    justify-content: center;\n    align-items: center;\n}\n\n.taskList {\n    width: 50vw;\n    padding: 20px;\n    background: white;\n    box-shadow: 3px 3px 8px grey, -3px -3px 8px grey;\n    border-radius: 5px;\n\n    .taskItem {\n        padding: $lineSpacing 0 $lineSpacing 0;\n\n        #taskComplete {\n            @include styleButton(blue, 14%);\n        }\n        #taskDelete {\n            @include styleButton(red, 10%);\n        }\n    }\n\n    .newTaskInput {\n        width: 70%;\n        background: whitesmoke;\n        border: gainsboro solid 1px;\n        border-radius: 5px;\n        margin-top: $lineSpacing;\n        padding: $lineSpacing 0 $lineSpacing 5px;\n    }\n\n    #newTask {\n        @include styleButton(black, 25%);\n        margin-top: $lineSpacing;\n        font-size: 2vw;\n    }\n}"},"lineCount":null}},"error":null,"hash":"9b45bf482d8549708f895e887c4efd81","cacheData":{"env":{}}}